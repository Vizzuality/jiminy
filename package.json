{
  "name": "jiminy",
  "version": "0.1.0",
  "description": "Library for inferring which type of data visualization can be rendered from a JSON dataset, and with which data field(s)",
  "main": "dist/jiminy.js",
  "scripts": {
    "build": "NODE_ENV=production babel ./src --out-dir ./dist && webpack -p",
    "start": "webpack --progress --colors --watch",
    "lint": "eslint .",
    "test": "karma start karma.config.js",
    "prepublish": "npm run build"
  },
  "keywords": [
    "vizzuality",
    "jiminy",
    "data",
    "visualization",
    "inferrer",
    "recommendation"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/Vizzuality/jiminy.git"
  },
  "author": "Cl√©ment Prod'homme <contact@clementprodhomme.fr>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-loader": "^6.2.3",
    "babel-preset-latest": "^6.22.0",
    "chai": "^3.5.0",
    "eslint": "^3.16.1",
    "eslint-config-vizzuality": "^1.0.3",
    "eslint-loader": "^1.3.0",
    "karma": "^1.5.0",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-webpack": "^2.0.2",
    "mocha": "^3.2.0",
